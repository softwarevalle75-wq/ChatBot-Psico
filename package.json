{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "base-bailey-json",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "lint": "eslint . --no-ignore",
    "dev": "npm run lint && nodemon ./src/app.js",
    "bot": "node ./src/app.js",
    "start": "node start-all.js",
    "web": "node ./web/server.js",
    "dev-all": "npm run lint && nodemon ./start-all.js",
    "start-all": "node ./start-all.js",
    "test": "vitest run",
    "gen:pdf": "node ./scripts/generatePdf.js",
    "gen:pdf:ghq12": "node ./scripts/generatePdf.js --type ghq12",
    "gen:pdf:dass21": "node ./scripts/generatePdf.js --type dass21",
    "gen:pdf:both": "npm run gen:pdf:ghq12 && npm run gen:pdf:dass21",
    "build" : "npm i && npx prisma generate && npx prisma migrate dev --name init && npm run dev-all"
  },
  "prisma": {
    "seed": "node prisma/seed/seed.js"
  },
  "keywords": [],
  "dependencies": {
    "@builderbot/bot": "1.3.4",
    "@builderbot/database-mysql": "1.3.4",
    "@formkit/tempo": "^0.1.2",
    "@prisma/client": "6.18.0",
    "bcrypt": "^6.0.0",
    "builderbot-provider-sherpa": "^0.0.8",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "openai": "^5.11.0",
    "path": "^0.12.7",
    "pdfkit": "^0.17.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-plugin-builderbot": "latest",
    "nodemon": "^3.1.0",
    "prisma": "6.18.0",
    "vitest": "^2.1.9"
  },
  "author": "",
  "license": "ISC",
  "overrides": {
    "sharp": "0.34.3"
  }
}
